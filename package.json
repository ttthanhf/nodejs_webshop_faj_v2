  "main": "server.js",
  "scripts": {
    "test": "nodemon ./test/test.js",
    "start": "nodemon src/server.js",
    "scss-main": "node-sass --watch src/resources/scss/layouts/main.scss src/public/css/main.css",
    "scss-index": "node-sass --watch src/resources/scss/index/index.scss src/public/css/index.css",
    "scss-products": "node-sass --watch src/resources/scss/index/products.scss src/public/css/products.css",
    "scss-auth": "node-sass --watch src/resources/scss/index/auth.scss src/public/css/auth.css",
    "scss-contact": "node-sass --watch src/resources/scss/index/contact.scss src/public/css/contact.css",
    "scss-about": "node-sass --watch src/resources/scss/index/about.scss src/public/css/about.css",
    "scss-staff-layout": "node-sass --watch src/resources/scss/layouts/staff-layout.scss src/public/css/staff-layout.css",
    "scss-staff-products-cur": "node-sass --watch src/resources/scss/staff/products-cur.scss src/public/css/staff-products-cur.css",
    "scss-staff-products-list": "node-sass --watch src/resources/scss/staff/products-list.scss src/public/css/staff-products-list.css",
    "scss-all": "npm-run-all -p scss-main scss-index scss-products scss-auth scss-contact scss-about scss-staff-layout scss-staff-products-cur scss-staff-products-list",
    "dev": "npm-run-all -p scss-all start"
  },
